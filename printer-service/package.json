{
  "name": "at-restaurant-printer-service",
  "version": "3.0.0",
  "description": "Production WebSocket printer service for AT Restaurant POS - Supports Windows 32-bit & 64-bit",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "install-printer-32": "npm install printer --build-from-source --target_arch=ia32",
    "install-printer-64": "npm install printer --build-from-source --target_arch=x64",
    "test": "node -e \"const p = require('printer'); console.log('Printers:', p.getPrinters());\""
  },
  "dependencies": {
    "express": "^4.18.2",
    "ws": "^8.18.0",
    "cors": "^2.8.5"
  },
  "optionalDependencies": {
    "printer": "^0.4.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "printer",
    "thermal-printer",
    "pos",
    "websocket",
    "receipt",
    "windows",
    "production"
  ],
  "author": "AT Restaurant",
  "license": "MIT"
}